}

.cardgame{ text-align: center;
}

.contenitoregame{display: grid;
                grid-template-columns:auto auto auto ;
                margin: 10px;
                row-gap: 10px;
}
/* preordini */
.preordini a{ text-decoration: none;
              color: black;
}

.preordini{text-align: center;
}

.contenitorepreordini{ display: grid;
                       grid-template-columns: 1fr 1fr 1fr 1fr;
                       margin: 10px;
